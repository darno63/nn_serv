# Copy to .env and adjust values before running locally or in CI.
APP_MODULE=src.main:app
LOG_LEVEL=info
MODEL_CONFIG=configs/model.example.yaml
MODEL_DATA_DIR=/models
# Optional: token for accessing gated Hugging Face models
HF_TOKEN=
# CORS origins (comma separated)
ALLOWED_ORIGINS=http://localhost:5173
# API prefix exposed by FastAPI
API_PREFIX=/api
# Directory inside the container to store generated videos
OUTPUT_VIDEO_DIR=outputs
